    "components": {
      "buttons": {
        "primary": {
          "className": "bg-gradient-to-r from-blue-600 to-green-600 hover:from-blue-700 hover:to-green-700 text-white",
          "description": "Main action buttons with gradient background"
        },
        "secondary": {
          "className": "bg-white text-blue-600 border-blue-200 hover:bg-blue-50",
          "description": "Secondary buttons with outline style"
        },
        "danger": {
          "className": "bg-white text-red-600 border-red-200 hover:bg-red-50",
          "description": "Destructive action buttons"
        },
        "warning": {
          "className": "bg-gradient-to-r from-orange-500 to-red-500 hover:from-orange-600 hover:to-red-600",
          "description": "Warning or urgent action buttons"
        }
      },
      "cards": {
        "default": {
          "className": "border-2 border-gray-100 shadow-sm hover:shadow-md transition-shadow",
          "description": "Standard card component"
        },
        "primary": {
          "className": "border-2 border-blue-100 shadow-sm hover:border-blue-300 transition-colors",
          "description": "Primary themed cards"
        },
        "success": {
          "className": "border-2 border-green-100 shadow-sm",
          "description": "Success state cards"
        },
        "warning": {
          "className": "border-2 border-orange-100 shadow-sm",
          "description": "Warning state cards"
        },
        "elevated": {
          "className": "border-2 border-blue-100 shadow-xl",
          "description": "Elevated cards for important content"
        }
      },
      "badges": {
        "inStock": {
          "className": "bg-green-100 text-green-800",
          "description": "In stock status badge"
        },
        "lowStock": {
          "className": "bg-orange-100 text-orange-800",
          "description": "Low stock warning badge"
        },
        "outOfStock": {
          "className": "bg-red-100 text-red-800",
          "description": "Out of stock alert badge"
        },
        "category": {
          "className": "bg-blue-50 text-blue-700",
          "description": "Category classification badge"
        },
        "status": {
          "delivered": "bg-green-100 text-green-800",
          "shipped": "bg-blue-100 text-blue-800",
          "pending": "bg-orange-100 text-orange-800"
        }
      },
      "inputs": {
        "default": {
          "className": "border-gray-300 focus:border-blue-500 focus:ring-blue-500",
          "description": "Standard input field styling"
        },
        "withIcon": {
          "className": "pl-10 border-gray-300 focus:border-blue-500",
          "description": "Input fields with left-side icons"
        }
      },
      "navigation": {
        "header": {
          "className": "bg-white border-b shadow-sm",
          "description": "Main navigation header"
        },
        "navLink": {
          "active": "text-blue-600 font-medium",
          "inactive": "text-gray-600 hover:text-blue-600 transition-colors"
        }
      }
    },
    "layouts": {
      "backgrounds": {
        "main": {
          "className": "min-h-screen bg-gradient-to-br from-blue-50 via-white to-green-50",
          "description": "Main application background"
        },
        "auth": {
          "className": "min-h-screen bg-gradient-to-br from-blue-50 via-white to-green-50 flex items-center justify-center",
          "description": "Authentication pages background"
        },
        "white": {
          "className": "bg-white",
          "description": "Clean white background for cards and sections"
        }
      },
      "containers": {
        "main": {
          "className": "container mx-auto px-4",
          "description": "Main content container"
        },
        "narrow": {
          "className": "w-full max-w-md mx-auto",
          "description": "Narrow container for forms"
        },
        "wide": {
          "className": "w-full max-w-2xl mx-auto",
          "description": "Wide container for detailed forms"
        }
      },
      "grids": {
        "responsive": {
          "className": "grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6",
          "description": "Responsive grid layout"
        },
        "stats": {
          "className": "grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6",
          "description": "Statistics cards grid"
        },
        "twoColumn": {
          "className": "grid lg:grid-cols-3 gap-6",
          "description": "Two column layout with sidebar"
        }
      }
    },
    "states": {
      "stock": {
        "inStock": {
          "color": "green",
          "icon": "CheckCircle",
          "badge": "bg-green-100 text-green-800",
          "description": "Medicine is available in sufficient quantity"
        },
        "lowStock": {
          "color": "orange",
          "icon": "AlertTriangle",
          "badge": "bg-orange-100 text-orange-800",
          "description": "Medicine quantity is below minimum threshold"
        },
        "outOfStock": {
          "color": "red",
          "icon": "Clock",
          "badge": "bg-red-100 text-red-800",
          "description": "Medicine is not available"
        }
      },
      "orders": {
        "delivered": {
          "color": "green",
          "badge": "bg-green-100 text-green-800"
        },
        "shipped": {
          "color": "blue",
          "badge": "bg-blue-100 text-blue-800"
        },
        "pending": {
          "color": "orange",
          "badge": "bg-orange-100 text-orange-800"
        }
      }
    },
    "icons": {
