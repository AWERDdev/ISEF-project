        "shadow": "transition-shadow duration-200",
        "transform": "transition-transform duration-200",
        "all": "transition-all duration-200"
      },
      "hover": {
        "scale": "hover:scale-105",
        "shadow": "hover:shadow-lg",
        "opacity": "hover:opacity-80"
