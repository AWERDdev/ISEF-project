        "medical": ["Package", "Pill", "Heart", "Activity"],
        "navigation": ["Home", "Search", "Bell", "User"],
        "actions": ["Plus", "Edit", "Trash2", "Download", "Upload"],
        "status": ["CheckCircle", "AlertTriangle", "Clock", "XCircle"],
        "interface": ["Eye", "EyeOff", "MoreHorizontal", "ChevronDown"]
